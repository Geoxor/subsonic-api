{
  "name": "subsonic-api",
  "version": "1.1.1",
  "description": "",
  "license": "MIT",
  "author": "<mail@henrygressmann.de>",
  "type": "module",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "./lib/**/*"
  ],
  "scripts": {
    "watch": "npm run build:esbuild -- --watch",
    "build": "npm run build:esbuild && npm run types",
    "build:esbuild": "esbuild src/*.ts --minify --bundle --splitting --chunk-names=[name]-[hash] --outdir=lib --platform=neutral  --external:crypto --external:md5 --external:node-fetch --target=node16 --format=esm",
    "dev": "node --loader=ts-node/esm --no-warnings src/index.ts",
    "release": "release-it",
    "test": "vitest",
    "types": "tsc -p tsconfig.build.json",
    "preversion": "npm run build"
  },
  "devDependencies": {
    "@explodingcamera/eslint-config": "^26.2.1",
    "@types/md5": "^2.3.2",
    "@types/node": "^20.5.6",
    "esbuild": "^0.19.2",
    "eslint": "^8.48.0",
    "np": "^8.0.4",
    "release-it": "^16.1.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vitest": "^0.34.3"
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release v${version}"
    },
    "github": {
      "release": true
    }
  },
  "dependencies": {
    "md5": "^2.3.0",
    "node-fetch": "^3.3.2"
  }
}
